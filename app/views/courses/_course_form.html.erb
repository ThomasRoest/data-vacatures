
<%= icon('user') %>
<%= icon('home') %>
<%= icon('list-ul') %>
<%= icon('graduation-cap') %>
	
<%= simple_form_for @course, html: {novalidate: true} do |f| %>
	<%= f.input :title %>
	<%= f.input :description %>
	<%= f.input :category, as: :select, 
	            collection: course_categories %>
	<%= f.input :course_url %>
	 
	<%= f.input :company %>
	<%= f.button :submit, class: 'btn btn-default-submit btn-success' %>
<% end %>
